<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Solicitacao.
    -->
    <changeSet id="20210322194346-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="id_solicitacao_id"
                                 baseTableName="solicitacao"
                                 constraintName="fk_solicitacao_id_solicitacao_id"
                                 referencedColumnNames="id"
                                 referencedTableName="resultado_validacao"/>

        <addForeignKeyConstraint baseColumnNames="usuario_id"
                                 baseTableName="solicitacao"
                                 constraintName="fk_solicitacao_usuario_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>

    </changeSet>
</databaseChangeLog>
