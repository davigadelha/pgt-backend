<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Usuario.
    -->
    <changeSet id="20210322194246-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="perfil_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario_perfil_id"
                                 referencedColumnNames="id"
                                 referencedTableName="perfil"/>

        <addForeignKeyConstraint baseColumnNames="uf_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario_uf_id"
                                 referencedColumnNames="id"
                                 referencedTableName="uf"/>

    </changeSet>
</databaseChangeLog>
